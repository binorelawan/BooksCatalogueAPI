{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MyDbConnection": "Server=tcp:<databaseServerName>.database.windows.net,1433;Database=<databaseName>;User ID=<IDUsername>;Password=<Password>;Encrypt=true;Connection Timeout=30;"
  },
  "AzureStorageConfig": {
    "AccountName": "<Storage Name>",
    "AccountKey": "<Key Storage>",
    "ImageContainer": "<Storage Container Name>"
  }
}
